<odoo>
    <record id="res_partner_form_inherit_duplicate_warning" model="ir.ui.view">
        <field name="name">res.partner.form.inherit.duplicate.warning</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet" position="before">
                <field name="duplicate_warning" invisible="not duplicate_warning" readonly="1"
                       class="alert alert-warning mb-0 p-3" nolabel="1"/>
            </xpath>
        </field>
    </record>
</odoo>
