<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_form_board_feet" model="ir.ui.view">
        <field name="name">product.template.form.board_feet</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <field name="weight" position="before">
                <field name="lumber_length"/>
                <field name="lumber_width"/>
                <field name="lumber_thickness"/>
                <field name="board_feet"/>
            </field>
        </field>
    </record>
</odoo>
