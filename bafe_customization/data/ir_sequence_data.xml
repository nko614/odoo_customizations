<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_purchase_order_unique" model="ir.sequence">
            <field name="name">Purchase Order Unique Key</field>
            <field name="code">purchase.order.unique</field>
            <field name="prefix">UNIQUE/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_purchase_order_line_unique" model="ir.sequence">
            <field name="name">Purchase Order Line Unique</field>
            <field name="code">purchase.order.line.unique</field>
            <field name="prefix">UNIQUE/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo> 